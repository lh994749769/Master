<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.master.kit.testcase.touchevent.TouchActivity"
    tools:showIn="@layout/app_bar_touch">
    <com.master.kit.testcase.touchevent.FirstViewGroup
        android:id="@+id/first"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <com.master.kit.testcase.touchevent.SecondViewGroup
            android:id="@+id/second"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <com.master.kit.testcase.touchevent.ThirdViewGroup
                android:id="@+id/third"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
            </com.master.kit.testcase.touchevent.ThirdViewGroup
                >
        </com.master.kit.testcase.touchevent.SecondViewGroup
            >
    </com.master.kit.testcase.touchevent.FirstViewGroup
        >

</RelativeLayout>
